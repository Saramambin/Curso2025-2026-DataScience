	rr:termType rr:Literal .

map:om_004 rml:reference "alcaldias" ;
	rdf:type rr:ObjectMap ;
	rr:termType rr:Literal .

map:om_005 rml:reference "tipo" ;
	rdf:type rr:ObjectMap ;
	rr:termType rr:Literal .

map:om_006 rdf:type rr:ObjectMap ;
	rr:constant metro:Observation ;
	rr:termType rr:IRI .

map:om_007 rml:reference "anio" ;
	rdf:type rr:ObjectMap ;
	rr:datatype xsd:gYear ;
	rr:termType rr:Literal .

map:om_008 rml:reference "mes" ;
